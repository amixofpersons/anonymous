<div class='sign-up-container'>
<h1 class="vertical-align">Log In </h1>
<%= flash.now[:error] %>

<%= form_for(:session, :url => sessions_path) do |f| %>
 <br>
 <%= f.text_field :user_name, placeholder: "User Name", class: "user-input" %>

<br>
 <%= f.password_field :user_password, placeholder: "Password", class: "user-input" %>
<br>
 <%= f.submit nil, :class => "new-user" %>
<% end %>
</div>
